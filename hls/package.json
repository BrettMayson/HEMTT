{
  "name": "hemtt-language-server",
  "description": "nano rust language server",
  "license": "MIT",
  "version": "1.5.0",
  "categories": [],
  "keywords": [
    "language-server",
    "tower-lsp"
  ],
  "repository": {
    "url": "https://github.com/brettmayson/hemtt"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "enabledApiProposals": [],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "grammars": [
      {
        "language": "sqf",
        "scopeName": "sqf",
        "path": "./languages/sqf.json"
      },
      {
        "language": "arma-config",
        "scopeName": "config",
        "path": "./languages/config.json"
      }
    ],
    "languages": [
      {
        "id": "sqf",
        "aliases": [
          "SQF",
          "sqf",
          "Arma SQF"
        ],
        "extensions": [
          ".sqf"
        ],
        "configuration": "./languages/sqf.configuration.json"
      },
      {
        "id": "arma-config",
        "aliases": [
          "Arma Config"
        ],
        "extensions": [
          ".ext",
          ".hpp",
          ".sqm"
        ],
        "filenames": [
          "config.cpp"
        ],
        "configuration": "./languages/config.configuration.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "hemtt-language-server",
      "properties": {
        "hemtt-language-server.trace.server": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "enumDescriptions": [
            "No traces",
            "Error only",
            "Full log"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint vscode-src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.88.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "glob": "^10.3.12",
    "mocha": "^10.4.0",
    "typescript": "^5.4.5",
    "vscode-test": "^1.4.0",
    "vscode-uri": "^3.0.8"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
