        author = ECSTRING(common,ACETeam); \
        scope = 1; \
        scopeArsenal = 0; \
        scopeCurator = 0; \
        descriptionShort = CSTRING(itemName); \
        GVAR(tagID) = DOGTAGID; \
    }

